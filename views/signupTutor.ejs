<body class="text-center">
  <%tutorSignUp = true;%>
  <form class="form-signin" method="post" action="/signup">
    <img class="mb-4" src="images/Logo.png" width="125" height="125">
    <h1 class="h3 mb-3 font-weight-normal">Sign up as a tutor!</h1>
    <p><%=signupFailure%></p>
    <input type="username" name="username" class="form-control top" placeholder="Email" required autofocus autocomplete="off">
    <input type="password" name="password" class="form-control middle" placeholder="Password" required autocomplete="off">
    <input type="age" name="age" class="form-control middle" placeholder="age" required autocomplete="off">

    <button class="btn btn-lg btn-primary btn-block" type="submit">Submit</button>
    <p class="mt-5 mb-3 text-muted">&copy; Tannen Hall</p>
  </form>

</body>

